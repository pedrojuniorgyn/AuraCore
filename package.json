{
  "name": "aura_core",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.mjs,.js",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "NODE_OPTIONS='--max-old-space-size=8192' vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:e2e": "vitest run --dir tests/e2e --fileParallelism=false",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run tests/unit --coverage",
    "test:watch": "vitest --watch",
    "test:arch": "vitest run tests/architecture/",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:headed": "playwright test --headed",
    "test:playwright:debug": "playwright test --debug",
    "test:playwright:report": "playwright show-report",
    "validate": "npm run typecheck && npm run test:unit",
    "prepare": "husky",
    "docker:test:up": "docker-compose -f docker-compose.test.yml up -d sqlserver-test",
    "docker:test:down": "docker-compose -f docker-compose.test.yml down -v",
    "docker:test:logs": "docker-compose -f docker-compose.test.yml logs -f",
    "docker:test:restart": "npm run docker:test:down && npm run docker:test:up",
    "docker:test:init-db": "docker-compose -f docker-compose.test.yml run --rm db-init",
    "docker:test:migrate": "docker-compose -f docker-compose.test.yml run --rm migrations",
    "db:migrate:test": "drizzle-kit push",
    "test:integration:docker": "npm run docker:test:up && sleep 30 && npm run docker:test:init-db && npm run docker:test:migrate && (npm run test:integration; TEST_EXIT=$?; npm run docker:test:down; exit $TEST_EXIT)",
    "test:all": "npm run test:unit && npm run test:integration:docker",
    "test:playwright:strategic": "playwright test tests/e2e/strategic/",
    "test:playwright:mobile": "playwright test tests/e2e/strategic/mobile.spec.ts"
  },
